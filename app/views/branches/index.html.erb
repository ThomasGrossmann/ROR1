<div class="w-full">
  <div class="flex justify-between items-center">
    <h1 class="font-bold text-4xl">Branches</h1>
    <% if current_user.dean? %>
      <%= link_to "New branch", new_branch_path, class: button_style('create') %>
    <% end %>
  </div>

  <div id="branches" class="flex md:flex-row gap-4 flex-col">
    <%= render @branches %>
  </div>
</div>
