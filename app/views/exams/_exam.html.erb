<div id="<%= dom_id exam %>">
  <tr>
    <td class="p-2"><%= exam.theme %></td>
    <td class="p-2"><%= exam.branch.name %></td>
    <td class="flex flex-row items-center gap-4">
      <%= link_to "Edit", edit_exam_path(exam), class: button_style('edit') %>
      <%= button_to "Delete", exam, method: :delete, data: { confirm: "Are you sure?" }, class: button_style('delete') %>
    </td>
  </tr>
</div>
