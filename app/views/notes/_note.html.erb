<div id="<%= dom_id note %>">
  <div class="p-4 flex flex-row justify-between">
    <div class="flex flex-row gap-4 items-center">
      <span class="text-lg font-bold"><%= note.student.fullname %></span>
      <span>|</span>
      <span><%= note.branch.name %></span>
    </div>
    <div class="flex flex-row gap-4">
      <span>|</span>
      <span><%= note.note %></span>
    </div>
  </div>
</div>
